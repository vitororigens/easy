{
  "expo": {
    "name": "easy",
    "slug": "easy",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist",
      "bundleIdentifier": "com.easy.io.app",
      "buildNumber": "3",
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.easy.io.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "versionCode": 3,
      "runtimeVersion": "1.1.0"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "@react-native-firebase/messaging",
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "644c7760-a5b0-4044-bcd4-9288b16c6948"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/644c7760-a5b0-4044-bcd4-9288b16c6948"
    }
  }
}
